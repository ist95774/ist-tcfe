
R1 = 1.03055897104e3
R2 = 2.07633317652e3
R3 = 3.11228971368e3
R4 = 4.1814130542e3
R5 = 3.06747464193e3
R6 = 2.01035149284e3
R7 = 1.00587624853e3
Va = 5.1951993125
Id = 1.04139652596e-3

Kb = 7.11172371179e-3 % S 
Kc = 8.02979201109e3 % Ohm


A = [[1,0,0,0];[0,1-Kb*R3,Kb*R3,0];[0,-R3,R3+R4+R1,-R4];[0,0,-R4,R6+R7-Kc+R4]]
B = [Id; 0; -Va; 0]

C = A\B

Iaa = C(1,1)
Iab = C(2,1)
Iac = C(3,1)
Iad = C(4,1)


Vb = (Iab-Iac)*R3
Ic = Iad
Ib = Iab
Ib = Kb*Vb
Vc = Kc*Ic 